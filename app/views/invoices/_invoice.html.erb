<div id="<%= dom_id invoice %>">
  <p>
    <strong>Code:</strong>
    <%= invoice.code %>
  </p>

  <p>
    <strong>Discount:</strong>
    <%= invoice.discount %>
  </p>

  <p>
    <strong>Due date:</strong>
    <%= invoice.due_date %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= invoice.notes %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= invoice.status %>
  </p>

  <p>
    <strong>Tax:</strong>
    <%= invoice.tax %>
  </p>

  <p>
    <strong>Terms:</strong>
    <%= invoice.terms %>
  </p>

</div>
